{
  "name": "boilerplate",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "bower install --production --config.interactive=false && gulp build",
    "start": "gulp watch",
    "deploy:validation": "shipit validation deploy",
    "deploy:preprod": "shipit preprod deploy",
    "deploy:prod": "shipit prod deploy"
  },
  "dependencies": {
    "bower": "*",
    "coffee-script": "*",
    "gulp": "*",
